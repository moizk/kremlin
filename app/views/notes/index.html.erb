<h1>Listing notes</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Url</th>
    <th>User</th>
    <th>Ip address</th>
    <th>User agent</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @notes.each do |note| %>
  <tr>
    <td><%= note.title %></td>
    <td><%= note.url %></td>
    <td><%= note.user_id %></td>
    <td><%= note.ip_address %></td>
    <td><%= note.user_agent %></td>
    <td><%= link_to 'Show', note %></td>
    <td><%= link_to 'Edit', edit_note_path(note) %></td>
    <td><%= link_to 'Destroy', note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Note', new_note_path %>

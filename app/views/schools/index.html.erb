<h1>Listing schools</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Website</th>
    <th>Facebook</th>
    <th>Twitter</th>
    <th>Wiki</th>
    <th>Private</th>
    <th>Students</th>
    <th>Saf</th>
    <th>Budget</th>
    <th>Sga website</th>
    <th>Sga facebook</th>
    <th>Sga twitter</th>
    <th>Newspaper</th>
    <th>Competitor</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @schools.each do |school| %>
  <tr>
    <td><%= school.name %></td>
    <td><%= school.website %></td>
    <td><%= school.facebook %></td>
    <td><%= school.twitter %></td>
    <td><%= school.wiki %></td>
    <td><%= school.private %></td>
    <td><%= school.students %></td>
    <td><%= school.saf %></td>
    <td><%= school.budget %></td>
    <td><%= school.sga_website %></td>
    <td><%= school.sga_facebook %></td>
    <td><%= school.sga_twitter %></td>
    <td><%= school.newspaper %></td>
    <td><%= school.competitor_id %></td>
    <td><%= school.latitude %></td>
    <td><%= school.longitude %></td>
    <td><%= link_to 'Show', school %></td>
    <td><%= link_to 'Edit', edit_school_path(school) %></td>
    <td><%= link_to 'Destroy', school, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New School', new_school_path %>
